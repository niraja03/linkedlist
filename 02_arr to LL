#include <bits/stdc++.h>
using namespace std;

class Node{
public:
    int data;
    Node* next;

    Node(int val){
        data= val;
        next= nullptr;
    }
};

int main() {


    int arr[] = {3,4,5,6,4};
    int n= 5;

    //creating 1st node that is head
    Node* head = new Node(arr[0]);
    Node* temp = head;

    //loop through rest of array
    for (int i =0; i<n ; i++) {
        temp->next = new Node(arr[i]);
        temp = temp->next;
    }


    //print LL
    temp = head;
    while(temp!= nullptr) {
        cout<< temp-> data <<" ";
        temp = temp-> next;
    }
    return 0;
}
