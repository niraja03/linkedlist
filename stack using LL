#include <iostream>
using namespace std;

class Node {
public:
    int data;
    Node* next;
};

class StackList {
    Node* top;

public:
    StackList() { top = NULL; }

    void push(int x) {
        Node* temp = new Node();
        temp->data = x;
        temp->next = top;
        top = temp;
    }

    void pop() {
        if (top == NULL)
            cout << "Stack Underflow\n";
        else {
            Node* temp = top;
            top = top->next;
            delete temp;
        }
    }

    void peek() {
        if (top != NULL)
            cout << "Top element: " << top->data << endl;
    }
};

int main() {
    StackList s;
    s.push(10);
    s.push(20);
    s.peek();
    s.pop();
    s.peek();
}
